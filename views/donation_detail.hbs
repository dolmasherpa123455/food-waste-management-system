<div class='row'>
  <div class='container mt-5'>
    <div class='d-flex flex-row user-info'><img class='rounded-circle' src='data:image/png;base64,' width='50' />
      <div class='d-flex flex-column justify-content-start ml-2'><span
          class='d-block font-weight-bold name'
        >{{donation.creator}}</span><span class='date text-black-50'>{{donation.createdAt}}</span></div>
    </div>
    <div class='mt-2'>
      <p class='comment-text'><b>{{donation.title}}</b></p>
      <p class='comment-text'>{{donation.description}}</p>
      <p class='comment-text'>Location : {{donation.source_location}}</p>
      <p class='comment-text'>Can Be Used As : {{donation.suitable_for}}</p>
      <p class='comment-text'>Can Be Used As : {{donation.id}}</p>
      {{!-- <div class='p-2'>
        <form method='POST' action='/donation/{{id}}/comment'>
          <div class='mt-2 text-right'><button class='btn btn-primary btn-sm shadow-none' type='submit'>Chat</button></div>
        </form>
      </div> --}}
    </div>

    {{#each donation.comments}}
      <div class='p-2'>

        <div class='d-flex flex-row align-items-start row'>
          <img class='rounded-circle' src='data:image/png;base64,' width='40' />
          <div class='col-lg-6'>
            <p class='comment-text ml-3'><b>{{user.fullName}}</b> </p>
          </div>
          <div class='col-lg-12'>
            <p class='comment-text' style='margin-left: 55px ;'>{{message}}</p>
          </div>
        </div>
        <hr />
      </div>
    {{/each}}

    <div class='p-2'>
      <form method='POST' action='/donation/{{id}}/comment'>
        <input type='hidden' name='_csrf' value='{{csrfToken}}' />
        <div class='d-flex flex-row align-items-start'><img
            class='rounded-circle'
            src='data:image/png;base64,'
            width='40'
          /><textarea class='form-control ml-1 shadow-none textarea' name='message'></textarea></div>

        <div class='mt-2 text-right'><button class='btn btn-primary btn-sm shadow-none' type='submit'>Send</button></div>
      </form>

    </div>
  </div>

</div>

